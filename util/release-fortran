#!/usr/bin/env bash

lang=fortran
tag_prefix=  # No v prefix for Fortran
root=$(cd "$(dirname "$0")/.." && pwd)

update() (
  cp "$root/License" "$repo_dir/LICENSE"

  cd "$from_dir"
  cp \
    ReadMe.md \
    fpm.toml \
    "$repo_dir/"

  cp -r src test "$repo_dir/"
)

source "$(dirname "$0")/release-repo.bash"
